{
  "NHibernate": {
    "Common": {
      "Dialect": "NHibernate.Dialect.SQLiteDialect",
      "ShowSql": true
    },
    "Connection": {
      "DriverClass": "NHibernate.Driver.SQLite20Driver",
      "ConnectionString": "Data Source=nhibernate.s3db;Version=3;Page Size=1024;BinaryGUID=False;"
    },
    "Query": {
      "Substitutions": "true=1;false=0"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
